<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.otyon.gamesummary">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@android:style/Theme.NoTitleBar">
        <activity
            android:name=".activity.NewActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.OpeningSpriteActivity"/>
        <activity
            android:name=".activity.NewDetailActivity" />
        <activity
            android:name=".activity.InformationActivity" />
        <activity
            android:name=".activity.DictionaryActivity" />
        <activity
            android:name=".activity.BbsTopActivity" />
    </application>

</manifest>